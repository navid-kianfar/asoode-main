                    </g>
                </svg>
            </a>
        </div>
        <h6>@translate.Get("BOOZHAN_PRODUCT")</h6>
        <br>
        <a target="_blank" href="https://www.linkedin.com/company/asoode" class="linkedin">
            <i class="icon-linkedin2"></i>
        </a>&nbsp;
        <a target="_blank" href="https://t.me/AsoodeCom" class="telegram">
            <i class="icon-telegram"></i>
        </a>&nbsp;
    </div>
</div>
<script src="/scripts/jquery-3.4.1.min.js"></script>
<script src="/scripts/bootstrap.min.js"></script>
<script src="/scripts/layout.js"></script>
</body>
</html>