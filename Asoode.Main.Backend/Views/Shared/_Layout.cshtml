<!DOCTYPE html>
@using Asoode.Core.Contracts.General
@{ Layout = ""; }
@inject ITranslateBiz translate
<html dir="@ViewBag.Direction" lang="@ViewBag.Lang">
<head>
    <meta charset="utf-8"/>
    <meta content="text/html; charset=UTF-8" http-equiv="Content-Type"/>
    <meta content="IE=edge" http-equiv="X-UA-Compatible"/>
    <meta name="viewport" content="width=device-width,height=device-height,initial-scale=1,maximum-scale=1,user-scalable=no,shrink-to-fit=no"/>
    <meta name="description" content="@ViewData["PAGE_DESCRIPTION"]">
    <meta name="msapplication-TileColor" content="#da532c" />
    <meta name="theme-color" content="#ffffff" />
    <title>@ViewData["PAGE_TITLE"]</title>
    <link rel="stylesheet" href="~/stylesheets/bootstrap.@(ViewBag.Direction).min.css"/>
    <link rel="stylesheet" href="~/stylesheets/layout.css"/>
    <link rel="apple-touch-icon" sizes="180x180" href="/images/favicons/apple-touch-icon.png" />
    <link rel="icon" type="image/png" sizes="32x32" href="/images/favicons/favicon-32x32.png" />
    <link rel="icon" type="image/png" sizes="16x16" href="/images/favicons/favicon-16x16.png" />
    <link rel="manifest" href="/images/favicons/app.@(ViewBag.Lang).manifest.txt" />
    <link rel="mask-icon" href="/images/favicons/safari-pinned-tab.svg" color="#5bbad5" />
</head>
<body class="layout lang-@ViewBag.Lang dir-@ViewBag.Direction">
<div class="header">
    <div class="image">
        <nav>
            <div class="box">
                <div class="logo">
                    <a href="/">
                        <img src="/images/asoode-logo.svg" alt="logo">
                    </a>
                </div>
                <a class="link" href="/@ViewBag.Lang/why">
                    @translate.Get("WHY_ASOODE")
                </a>
                <a class="link" href="/@ViewBag.Lang/plans">
                    @translate.Get("PLANS")
                </a>
                <a class="link" href="/@ViewBag.Lang/about">
                    @translate.Get("ABOUT_US")
                </a>
            </div>
            <div class="box">
                <a class="link login" href="https://panel.asoode.com/login">
                    @translate.Get("LOGIN")
                </a>
                <a class="link bordered" href="https://panel.asoode.com/register">
                    @translate.Get("REGISTER")
                </a>
            </div>
        </nav>
    </div>

    <h1>@ViewData["PAGE_TITLE"]</h1>
</div>
@RenderBody()
<div class="footer">
    <div class="box asoode">
        <a class="logo" href="/">
            <img src="/images/asoode-logo.svg" alt="">
        </a>
        <a href="/@ViewBag.Lang/contact">@translate.Get("CONTACT_US")</a>
        <a href="javascript:void(0)">@translate.Get("JOB_OPPORTUNITIES")</a>
        <a href="/@ViewBag.Lang/faq">@translate.Get("FAQ")</a>
        <a href="/@ViewBag.Lang/blog">@translate.Get("BLOG")</a>
    </div>
    <div class="box middle">
        <div>
            @translate.Get("SHARIF_UNIVERSITY")<br>
            @translate.Get("COPY_RIGHT")&nbsp;&copy;@(DateTime.Now.Year)
        </div>
    </div>
    <div class="box boozhan">
        <div>
            <a target="_blank" class="logo" href="https://boozhan-sharif.ir">
                <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="29pt" height="22pt" viewBox="0 0 29 22" version="1.1">
                    <g id="surface1">
